{
  "kind": "collectionType",
  "collectionName": "Seiten",
  "info": {
    "name": "Seite",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "pluginOptions": {}
    },
    "Permalink": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "Inhalt": {
      "type": "dynamiczone",
      "components": [
        "text.formattierter-text",
        "interaktion.link",
        "text.abschnittstitel",
        "medien.bild"
      ]
    }
  }
}
